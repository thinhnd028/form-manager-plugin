[
  {
    "label": "1. Thông tin liên lạc ?",
    "fields": [
      {
        "name": "fullName",
        "label": "Họ và tên",
        "required": true,
        "dataFormat": "text"
      },
      {
        "name": "phone",
        "label": "Số điện thoại",
        "required": true,
        "dataFormat": "phone"
      },
      {
        "name": "email",
        "label": "Email",
        "required": true,
        "dataFormat": "email"
      }
    ]
  },
  {
    "label": "2. Chi phí ước tính của Quý khách hàng có thể đầu tư là bao nhiêu?",
    "fields": [
      {
        "name": "estimatedCost",
        "label": "Chi phí ước tính",
        "required": true,
        "dataFormat": "currency"
      }
    ]
  },
  {
    "name": "hasProperty",
    "label": "3. Bạn đã có mặt bằng hay chưa có mặt bằng?",
    "options": [
      "Đã có",
      "Chưa có"
    ],
    "required": true,
    "dataFormat": "choice"
  },
  {
    "name": "estimatedArea",
    "label": "4. Diện tích ước tính của mặt bằng hiện tại?",
    "options": [
      "Dưới 40m2",
      "Từ 40m2 đến 60m2",
      "Từ 60m2 đến 100m2",
      "Từ 100m2 đến 200m2",
      "Trên 200m2",
      "Không biết rõ diện tích"
    ],
    "required": true,
    "dataFormat": "choice"
  },
  {
    "label": "5. Khu vực dự định muốn mở quán?",
    "fields": [
      {
        "name": "province",
        "label": "Tỉnh/Thành phố",
        "dropdown": true,
        "required": true,
        "dataFormat": "choice",
        "optionSource": {
          "url": "http://localhost:1337/api/provinces?pagination[limit]=999",
          "labelField": "name"
        }
      },
      {
        "name": "commune",
        "label": "Phường/Xã",
        "dropdown": true,
        "required": true,
        "dataFormat": "choice",
        "optionSource": {
          "url": "http://localhost:1337/api/communes/active?pagination[limit]=100",
          "type": "strapi",
          "filters": {
            "active": true,
            "province.name": "{{province}}"
          },
          "labelField": "name"
        },
        "dependentField": "province"
      },
      {
        "name": "address",
        "label": "Địa chỉ",
        "required": true,
        "dataFormat": "text"
      }
    ]
  }
]
